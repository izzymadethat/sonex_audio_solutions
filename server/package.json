{
  "name": "@sonex/server",
  "version": "0.0.0",
  "description": "The most basic setup for express. Has the dependencies to run an express backend server.",
  "main": "index.js",
  "scripts": {
  "dev": "nodemon index.js",
  "start": "node index.js",
  "prisma:dev": "dotenv -e .env.local prisma generate",
  "prisma:prod": "dotenv -e .env.production prisma generate",
  "migrate": "npx prisma migrate dev",
  "db:reset": "npx prisma migrate reset",
  "db:deploy": "npx prisma migrate deploy",
  "prisma:studio": "npx prisma studio",
  "prisma:seed": "node prisma/seed.js"
},
  "keywords": [
    "express",
    "express",
    "template",
    "express",
    "basic"
  ],
  "author": "Isaiah Vickers",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.3",
    "nodemon": "^3.1.7"
  }
}
